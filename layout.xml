<?xml version="1.0" encoding="utf-8"?>
<layout>
  <row id="notificationbar" weight="1">
      <label id="notification" text="" color=""/>
  </row>
  <row id="header" weight="2">
    <toggle id="inside" text="INSIDE" onchange="insideChanged"/>
    <toggle id="outside" text="OUTSIDE" onchange="outsideChanged"/>
  </row>
  <row id="body" weight="10">
    <grid >
      <row>
        <button icon="vdown" ontap="Volume_Up"/>
        <button icon="vup" ontap="Volume_Down"/>
        <button icon="vmute" ontap="Mute/Unmute"/>
      </row>
      <row>
        <button id="auxOpt" text="Media Player"/>
      </row>
      <row>
        <button id="bluetooth" text="Bluetooth"/>
      </row>
      <row>
        <button id="aux" text="Television"/>
      </row>
    </grid>
  </row>
  <row id="footer" weight="1">
    <label id="" text="" color="black"/>
  </row>
</layout>